{
  "name": "social-network-app",
  "version": "1.0.0",
  "scripts": {
    "build:angular": "ng build",
    "build:admin": "cd node_modules/social-network-admin-rnymphaea && npm run build:gulp",
    "build:all": "npm run build:admin && npm run build:angular",
    "start:server": "node server/server.js",
    "start:dev": "concurrently \"ng serve\" \"npm run start:server\"",
    "start:prod": "npm run build:all && npm run start:server",
    "test:server": "jest server/tests/api.test.ts",
    "install:all": "npm install && npm run build:all"
  },
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "ws": "^8.14.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0",
    "social-network-admin-rnymphaea": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "gulp-pug": "^5.0.0",
    "gulp-terser": "^2.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.0.0",
    "supertest": "^6.0.0",
    "concurrently": "^7.0.0"
  }
}
